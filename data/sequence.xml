<odoo>
    <record id="seq_admitted_id" model="ir.sequence">
        <field name="name">Forecast ID</field>
        <field name="code">sale.forecast</field>
        <field name="prefix">SF/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>  

      <record id="seq_purchase_forecast" model="ir.sequence">
        <field name="name">Purchase Forecast</field>
        <field name="code">purchase.forecast</field>
        <field name="prefix">FCAST/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record> 

       <record id="seq_mrf_forecast" model="ir.sequence">
        <field name="name"> Mrf Forecast</field>
        <field name="code">mrf.forecast.sequence</field>
        <field name="prefix">MF/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record> 
</odoo>